<Window x:Class="WpfRecommendarionSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfRecommendarionSystem"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525"
        xmlns:vm="clr-namespace:RecomendationModel.ViewModel;assembly=RecomendationModel">
    <Grid>
        <Grid.DataContext>
            <vm:MainViewModel/>
        </Grid.DataContext>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Background="LightCyan" Height="199" Width="317" ItemsSource="{Binding TicketEvents}">
        </DataGrid>
    </Grid>
</Window>
