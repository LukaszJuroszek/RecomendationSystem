<Window x:Class="WpfRecommendarionSystem.ShowTicketWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfRecommendarionSystem"
        mc:Ignorable="d"
        Title="ShowTicketWindow" Height="300" Width="300"
        xmlns:vm="clr-namespace:RecomendationModel.ViewModel;assembly=RecomendationModel">
    <Grid>
        <Grid.DataContext>
            <vm:ShowTicketViewModel/>
        </Grid.DataContext>
        <TextBox x:Name="showTextBox" Text="{Binding TicketEvent.Title}" Margin="0,0,187,194"/>
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="217,249,0,0" Click="okButton_Click"/>
    </Grid>
</Window>
